{% extends "layout/layout.njk" %}

{% block content %}

{# <pre>
    {{ page | dump('\t') }}
</pre> #}

<div class="container-fluid page-title text-white text-center py-5">
    <h1 class="mb-0">{{page.title}}</h1>
</div>
<div class="container py-5">
    <div class="row justify-content-between">
        <div class="col-md-7">
            {{ macro.contents(page.contents['row1.col1']) }}
        </div>
        <div class="col-md-4 mt-4 mt-md-0">
            {{ macro.contents(page.contents['row1.col2']) }}
        </div>
    </div>


    <div id="contact" class="row justify-content-between">
        <div class="col-md-7 mt-4">
            {{ macro.contents(page.contents['row2.col1']) }}
        </div>
    </div>
</div>
{% endblock %}
